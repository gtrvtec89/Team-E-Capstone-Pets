@model test.TBreed

@{
	ViewBag.Title = "Edit";
}

<h2>Breeds</h2>


@using (Html.BeginForm()) {
	@Html.AntiForgeryToken()

<div class="body-content">
	<div class="form-horizontal">
		<h4>Edit Breed</h4>
		<hr />
		@Html.ValidationSummary(true, "", new { @class = "text-danger" })
		@Html.HiddenFor(model => model.intBreedID)

		<div class="form-group row">
			@Html.LabelFor(model => model.strBreedName, "Breed", htmlAttributes: new { @class = "control-label col-md-2" })
			<div class="col-md-10">
				@Html.EditorFor(model => model.strBreedName, new { htmlAttributes = new { @class = "form-control" } })
				@Html.ValidationMessageFor(model => model.strBreedName, "", new { @class = "text-danger" })
			</div>
		</div>

		<div class="form-group row">
			<div class="col-md-offset-2 col-md-10">
				<input type="submit" value="Save" class="btn btn-default" />
			</div>
		</div>
	</div>
	<div>
		@Html.ActionLink("Back to List", "Index")
	</div>
</div>

		}



		@section Scripts {
			@Scripts.Render("~/bundles/jqueryval")
		}
