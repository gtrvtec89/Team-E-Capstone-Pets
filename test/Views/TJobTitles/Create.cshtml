@model test.TJobTitle

@{
    ViewBag.Title = "Create";
}

<h2>Job Descriptions</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
<div class="body-content">
	<div class="form-horizontal">
		<h4>Add a New Job Description</h4>
		<hr />
		@Html.ValidationSummary(true, "", new { @class = "text-danger" })
		<div class="form-group row">
			@Html.LabelFor(model => model.strJobTitleDesc, "Job Description",  htmlAttributes: new { @class = "control-label col-md-2" })
			<div class="col-md-10">
				@Html.EditorFor(model => model.strJobTitleDesc, new { htmlAttributes = new { @class = "form-control" } })
				@Html.ValidationMessageFor(model => model.strJobTitleDesc, "", new { @class = "text-danger" })
			</div>
		</div>

		<div class="form-group row">
			<div class="col-md-offset-2 col-md-10">
				<input type="submit" value="Create" class="btn btn-default" />
			</div>
		</div>
	</div>
	<div>
		@Html.ActionLink("Back to List", "Index")
	</div>
</div>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
