@model test.TJobTitle

<h2> Update Job Title </h2>

@using (Html.BeginForm("Update", "TJobTitles", FormMethod.Post, new { role = "form", id = "updateForm" })) {
	@Html.ValidationSummary(true)
	@Html.AntiForgeryToken()

	<div class="form-group row">
		@Html.HiddenFor(model => model.intJobTitleID)
		@Html.LabelFor(model => model.strJobTitleDesc, "Job Title:")
		@Html.TextBoxFor(model => model.strJobTitleDesc, new { @class = "form-control" })
	</div>


	<button type="submit" class="btn-primary">Update</button>

	@Html.ActionLink("Back To List", "Index", null, new { @style = "margin-left:50px; font-weight:bold;" })
	<div id="msg"></div>
}

