@model test.TServiceType



<h2> Update Service Type </h2>

@using (Html.BeginForm("Update", "TServiceTypes", FormMethod.Post, new { role = "form", id = "updateForm" })) {
	@Html.ValidationSummary(true)
	@Html.AntiForgeryToken()

	<div class="form-group">
		@Html.HiddenFor(model => model.intServiceTypeID)
		@Html.LabelFor(model => model.strServiceType, "Service Type:")
		@Html.TextBoxFor(model => model.strServiceType, new { @class = "form-control" })
	</div>
	

	<button type="submit" class="btn-primary">Update</button>

	@Html.ActionLink("Back To List", "Index", null, new { @style = "margin-left:50px; font-weight:bold;" })
	<div id="msg"></div>
}
