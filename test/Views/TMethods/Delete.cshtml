@model test.TMethod

<h2> Remove Medication Method </h2>
<h3>Are you sure you want to delete this?</h3>
<div>
	<table class="table table-responsive">
		<tr>
			<td>@Html.LabelFor(model => model.strMethod, "Medication Method:")</td>
			<td>@Html.DisplayFor(a => a.strMethod)</td>
		</tr>

	</table>
</div>


@using (Html.BeginForm("Delete", "TMethods", FormMethod.Post, new { id = "deleteForm" })) {
	<p>
		@Html.AntiForgeryToken()
		@Html.HiddenFor(a => a.intMethodID)
		<input type="submit" value="Delete" style="background-color:red; color:white" />
	</p>
}

